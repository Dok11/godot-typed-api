{
  "name": "godot-typed-api",
  "version": "4.4.3-stable",
  "type": "module",
  "exports": { ".": { "types": "./index.d.ts" } },
  "types": "./index.d.ts",
  "description": "Strictly modular TypeScript types for the Godot API.",
  "keywords": [
    "godot",
    "typescript",
    "types",
    "dts",
    "typings",
    "gamedev"
  ],
  "author": "",
  "license": "ISC",
  "files": [
    "index.d.ts",
    "src-template",
    "gen/current"
  ],
  "sideEffects": false,
  "publishConfig": { "access": "public" },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json",
    "test": "vitest --run",
    "fetch": "node dist/scripts/fetch-godot.js",
    "gen": "node dist/scripts/generate.js",
    "set-current": "node dist/scripts/build.js",
    "regen": "npm run gen && npm run set-current",
    "pack:local": "npm pack",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dok11/godot-typed-api.git"
  },
  "bugs": {
    "url": "https://github.com/Dok11/godot-typed-api/issues"
  },
  "homepage": "https://github.com/Dok11/godot-typed-api#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "fast-xml-parser": "^5.2.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "undici": "^7.15.0",
    "vitest": "^3.2.4",
    "zod": "^4.1.5"
  }
}
